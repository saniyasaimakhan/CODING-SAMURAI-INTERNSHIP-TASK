    {
      "cell_type": "code",
      "source": [
        "# Function to recommend top-rated movies in a selected genre\n",
        "def recommend_movies_by_genre(genre_name, min_ratings=30, top_n=10):\n",
        "    genre_movies = movies_genres[movies_genres['genre'] == genre_name]\n",
        "    genre_data = pd.merge(genre_movies, ratings, on='movieId')\n",
        "\n",
        "    genre_ratings = genre_data.groupby('title').agg({'rating': ['mean', 'count']})\n",
        "    genre_ratings.columns = ['average_rating', 'rating_count']\n",
        "\n",
        "    filtered = genre_ratings[genre_ratings['rating_count'] >= min_ratings]\n",
        "    top_recommendations = filtered.sort_values('average_rating', ascending=False).head(top_n)\n",
        "\n",
        "    print(f\"\\nTop {top_n} Movies in Genre: {genre_name}\")\n",
        "    print(top_recommendations)\n",
        "\n",
        "    # Optional: Plot\n",
        "    plt.figure(figsize=(10,6))\n",
        "    sns.barplot(y=top_recommendations.index, x=top_recommendations['average_rating'], palette=\"magma\")\n",
        "    plt.title(f\"Top {top_n} Movies in '{genre_name}' Genre\")\n",
        "    plt.xlabel(\"Average Rating\")\n",
        "    plt.ylabel(\"Movie Title\")\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n",
        "# Example usage:\n",
        "recommend_movies_by_genre(\"Action\", top_n=5)\n"
      ],
